javascript:(function()%20%7B%20%20%20%20%20const%20adSelectors%20=%20%5B%20%20%20%20%20%20%20%20%20'%5Bid*=%22ad%22%5D',%20'%5Bclass*=%22ad%22%5D',%20'%5Bid*=%22banner%22%5D',%20'%5Bclass*=%22banner%22%5D',%20%20%20%20%20%20%20%20%20'%5Bclass*=%22sponsored%22%5D',%20'%5Bclass*=%22promo%22%5D',%20'%5Bid*=%22popup%22%5D',%20'%5Bclass*=%22popup%22%5D',%20%20%20%20%20%20%20%20%20'%5Bid*=%22google_ads%22%5D',%20'%5Bclass*=%22google_ads%22%5D',%20'%5Bid*=%22adsense%22%5D',%20'%5Bclass*=%22adsense%22%5D',%20%20%20%20%20%20%20%20%20'%5Bclass*=%22advertisement%22%5D',%20'%5Bid*=%22advertisement%22%5D'%20%20%20%20%20%5D;%20%20%20%20%20%20adSelectors.forEach(selector%20=%3E%20%7B%20%20%20%20%20%20%20%20%20document.querySelectorAll(selector).forEach(el%20=%3E%20el.remove());%20%20%20%20%20%7D);%20%7D)();
